{
  "name": "react-redux-boilerplate",
  "description": "React + Redux boilerplate with Webpack 3, ES2016, react-router and Bulma.",
  "version": "3.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "predev": "babel-node buildScripts/devMessage.js",
    "dev": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node buildScripts/devServer.js",
    "lint": "esw src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel start localtunnel",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prodMessage": "babel-node buildScripts/prodMessage.js",
    "preprod": "npm-run-all prodMessage clean-dist lint",
    "prod": "babel-node buildScripts/build.js",
    "postprod": "babel-node buildScripts/prodServer.js",
    "test": "mocha buildScripts/testSetup.js \"src/**/*.test.js\" -r jsdom-global/register",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "font-awesome": "^4.7.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "3.0.0-beta.7",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "css-loader": "^0.28.4",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^3.12.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "7.0.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "localtunnel": "^1.8.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^3.1.2",
    "open": "^0.0.5",
    "prop-types": "^15.6.2",
    "react-test-renderer": "^16.4.2",
    "redux-immutable-state-invariant": "^1.2.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-md5-hash": "^0.0.5"
  }
}
